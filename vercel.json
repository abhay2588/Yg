{
  "version": 2,
  "builds": [
    {
      "src": "index.php",
      "use": "vercel-php"
    }
  ],
  "routes": [
    {
      "src": "/proxy/(.*)",
      "dest": "https://your-proxy-domain.example/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php?get=$1"
    }
  ],
  "regions": ["sin1"]
}
